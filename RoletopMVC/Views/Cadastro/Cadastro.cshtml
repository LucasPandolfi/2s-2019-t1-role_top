<main>
<form name="formteste" action='@Url.Action("CadastrarCliente", "Cadastro")' method="POST">
        <h1>Cadastro</h1>

    <div cadastro>

        <div class="email"> 
            <input type="email" name="email" placeholder="Informe seu e-mail" required="required">
        </div>

        <div class="senha">
            <input type="password" name="email" placeholder="Crie uma senha" required="required">
        </div>

        <div class="cpf/cnpj">
            <input type="number" name="cpf/cnpj" placeholder="Informe seu cpf/cnpj" required="required">
        </div>

        <div  class="nome">
            <input type="text" name="nome" placeholder="Informe seu nome" required="required"> 
        </div>

        <div class="data de nascimento">
            <input type="number" name="data de nascimento" placeholder="21/05/2001" required="required">
        </div>

        <div class="telefone">
            <input type="number" name="telefone" placeholder="(11) 98415-5212">
        </div>

        <div class="criar">
            <input type="submit" name="criar" value="Criar">
        </div>
    </div>
</form>
</main>